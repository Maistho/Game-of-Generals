<Window x:Class="Game_of_Generals.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="845" Width="920">
    <Grid Name="pnlMainGrid" Background="Beige">
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="800"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="900"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Name="pnlOverlayGrid" Grid.Column="1" Grid.Row="1" Panel.ZIndex="100">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Rectangle Name="switchRectangle" Grid.Row="0" Grid.RowSpan="2" Fill="Black" Visibility="Hidden"/>
            <Grid Name="pnlP1PiecesGrid" Grid.Row="0" Visibility="Hidden">
            </Grid>
            <Grid Name="pnlP2PiecesGrid" Grid.Row="1" Visibility="Hidden">
            </Grid>
        </Grid>
        <Grid Name="pnlBoardGrid" Grid.Column="1" Grid.Row="1">
        </Grid>
        <Button Name="finishButton" Grid.Column="1" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Top"/>
    </Grid>
</Window>
